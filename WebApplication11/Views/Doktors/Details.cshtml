@model WebApplication11.DataAccess.Doktor

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>
<hr />
<br />
<div> 
    <div class="text-center">
        <h3>@Html.DisplayFor(model => model.AdSoyad)</h3>
    </div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Cinsiyet:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cinsiyet)
        </dd>
        <dt class = "col-sm-2">
            Doğum Tarihi:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DogumTarihi)
        </dd>
        <dt class = "col-sm-2">
            Sifre:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sifre)
        </dd>
        <dt class = "col-sm-2">
            Çalıştığı Brim:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CalıstığıBrimNavigation.BölümIsmi)
        </dd>
        <dt class = "col-sm-2">
            İlgilenen Yönetici:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IlgilenenYöneticiNavigation.AdSoyad)
        </dd>
        <dt class = "col-sm-2">
            T.C No:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TCNo)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-light" asp-action="Index">Back to List</a>
</div>
