@model WebApplication11.DataAccess.HastaYakını

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>HastaYakını</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Ad Soyad:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AdSoyad)
        </dd>
        <dt class="col-sm-2">
            T.C No:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TCNo)
        </dd>
        <dt class="col-sm-2">
            Doğum Tarihi:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DogumTarihi)
        </dd>
        <dt class="col-sm-2">
            Hasta Ad Soyad:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HastaTCNavigation.Adsoyad)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.TCNo">Edit</a> 
    <a class="btn btn-light" asp-action="Index">Back to List</a>
</div>
